{
  "name": "mean",
  "version": "2.0.2",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "run-p --print-label clean serve \"build:* -- --watch\"",
    "deploy": "npm-run-all clean \"build:* -- --prod\" prod:serve",
    "clean": "del-cli dist/**/*",
    "build": "run-p clean \"build:*\"",
    "build:en": "ng build --output-path=dist/en/ --baseHref=/en/ --i18nLocale=en --i18nFile=src/locale/messages.en.xlf",
    "build:de": "ng build --output-path=dist/de/ --baseHref=/de/ --i18nLocale=de --i18nFile=src/locale/messages.de.xlf",
    "serve": "nodemon server",
    "prod:build": "npm-run-all clean \"build:* -- --prod\"",
    "prod:serve": "node server",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract-i18n": "ng xi18n --output-path locale && xliffmerge --profile src/xliffmerge.json en de"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.8",
    "@angular/cdk": "^8.2.1",
    "@angular/common": "^8.2.8",
    "@angular/compiler": "^8.2.8",
    "@angular/core": "^8.2.8",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "^8.2.8",
    "@angular/material": "^8.2.1",
    "@angular/platform-browser": "^8.2.8",
    "@angular/platform-browser-dynamic": "^8.2.8",
    "@angular/router": "^8.2.8",
    "angular-pipes": "^9.1.0",
    "angular-tree-component": "^8.4.0",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "core-js": "3.2.1",
    "cors": "^2.8.4",
    "dotenv": "^8.1.0",
    "events": "^3.0.0",
    "express": "^4.16.3",
    "express-async-handler": "^1.1.3",
    "express-jwt": "^5.3.1",
    "express-validation": "^1.0.2",
    "formidable": "^1.2.1",
    "hammerjs": "^2.0.8",
    "helmet": "^3.21.1",
    "http-errors": "^1.6.3",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.2.1",
    "method-override": "^3.0.0",
    "mongoose": "^5.7.1",
    "morgan": "^1.9.1",
    "ng2-dragula": "^2.1.1",
    "nodemon": "^1.17.5",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "rxjs": "6.5.3",
    "rxjs-compat": "^6.0.0-rc.0",
    "swagger-ui-express": "^4.1.1",
    "webpack-dev-server": "^3.8.1",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.6",
    "@angular/cli": "^8.3.6",
    "@angular/compiler-cli": "^8.2.8",
    "@angular/language-service": "^8.2.8",
    "@types/jasmine": "~3.4.0",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~12.7.8",
    "codelyzer": "^5.0.1",
    "del-cli": "^3.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.0",
    "karma-jasmine": "~2.0.1",
